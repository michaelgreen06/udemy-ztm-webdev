  var fun=5;
  function funFunction(){
    //child scope
    var fun="hello";
    console.log(1, fun);
  }

  function funnerFunction(){
    //child scope
    var fun="bye";
    console.log(2, fun);
  }


  function funnestFunction(){
    //child scope
    var fun="ahhhhh";
    console.log(3, fun);
  }

  console.log("window", fun);

  funFunction();
  funnerFunction();
  funnestFunction();
